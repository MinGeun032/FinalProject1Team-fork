spring.application.name=ConfigServer


#외부의 서비스(MSA를 이루고 있는 각각의 프로젝트 어플리케이셔) 들에게 정보를 제공해주기 위한 서버의 설정 정보
server.port=9000
cloud.config.server.git.uri=git@github.com:hg316/msa-config.git

#ssh-settiong 을 낙타기법으로...
spring.cloud.config.server.git.ignore-local-sshSettings=true
   
